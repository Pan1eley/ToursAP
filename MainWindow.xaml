<Window x:Class="ToursApp.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ToursApp"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="75"/>
            <RowDefinition/>
            <RowDefinition Height="58.04"/>
        </Grid.RowDefinitions>
        <Image Source="/Resources/logotip.png" Grid.Row="0" HorizontalAlignment="Left" Margin="30,0,0,0" Width="91"></Image>
        <TextBlock Text="Header" FontSize="30" HorizontalAlignment="Center" VerticalAlignment="Center" Height="39" Width="96"/>
        <Grid Background="#bae3e8" Panel.ZIndex="-2"/>
        <Grid Grid.Row="2" Background="#445c93"/>
        <Frame Grid.Row="1" Name="MainFrame"/>
        
        <Button Content="Back" Background="#fac716" Width="175" Height="NaN" Margin="620,22,0,23" HorizontalAlignment="Left" Name="BtnBack" Click="BtnBack_Click"/>
    </Grid>

</Window>
